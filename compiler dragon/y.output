    8 declarations: declarations VAR identifier_list ':' type ';'
    9             | %empty

   10 type: standard_type
   11     | ARRAY '[' INUM DOTDOT INUM ']' OF standard_type

   12 standard_type: INTEGER
   13              | REAL

   14 subprogram_declarations: subprogram_declarations subprogram_declaration ';'
   15                        | %empty

   16 $@5: %empty

   17 $@6: %empty

   18 subprogram_declaration: subprogram_head $@5 declarations $@6 subprogram_declarations compound_statement

   19 $@7: %empty

   20 subprogram_head: FUNCTION ID $@7 arguments ':' standard_type ';'

   21 $@8: %empty

   22 subprogram_head: PROCEDURE ID $@8 arguments ';'

   23 $@9: %empty

   24 arguments: $@9 '(' parameter_list ')'
   25          | %empty

   26 parameter_list: identifier_list ':' type
   27               | parameter_list ';' identifier_list ':' type

   28 compound_statement: BBEGIN optional_statements END

   29 optional_statements: statement_list
   30                    | %empty

   31 statement_list: statement
   32               | statement_list ';' statement

   33 statement: conditions
   34          | ifelse

   35 conditions: variable ASSIGNOP expression
   36           | procedure_statement
   37           | compound_statement
   38           | IF expression THEN conditions ELSE conditions
   39           | WHILE expression DO conditions
   40           | FOR ID ASSIGNOP expression TO expression DO conditions

   41 ifelse: IF expression THEN statement
   42       | IF expression THEN conditions ELSE ifelse

   43 variable: ID
   44         | ID '[' expression ']'

   45 procedure_statement: ID
   46                    | ID '(' expression_list ')'
   47                    | WRITE '(' expression_list ')'
   48                    | READ '(' expression_list ')'

   49 expression_list: expression
   50                | expression_list ',' expression

   51 expression: simple_expression
   52           | simple_expression RELOP simple_expression

   53 simple_expression: term
   54                  | ADDOP term
   55                  | simple_expression ADDOP term

   56 term: factor
   57     | term MULOP factor

   58 factor: ID
   59       | ID '(' expression_list ')'
   60       | ID '[' expression_list ']'
   61       | INUM
   62       | RNUM
   63       | '(' expression ')'
   64       | NOT factor


